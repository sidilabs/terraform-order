{
	"name": "terraform-order",
	"displayName": "Terraform Order",
	"description": "Tool to order variables in terraform code.",
	"publisher": "SiDiLabs",
	"icon": "./artifacts/images/tforder-logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/sidilabs/terraform-order"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:terraform-order.order"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "terraform-order.order",
				"title": "Order terraform variables"
			}
		],
		"keybindings": [
			{
				"command": "terraform-order.order",
				"key": "alt+shift+o"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"mnemonist": "^0.39.5"
	}
}
